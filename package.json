{
  "name": "animal-helper-token-contracts",
  "version": "1.0.0",
  "description": "Smart contracts for Animal Helper Token project on TON blockchain",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build:contracts": "tact --config ./tact.config.json",
    "build:scripts": "tsc",
    "build": "npm-run-all build:contracts build:scripts",
    "deploy": "npm run build && node dist/scripts/deploy-full.js",
    "deploy:mainnet": "npm run build && node dist/scripts/deploy-mainnet.js",
    "deploy:mainnet:dry-run": "npm run build && node dist/scripts/deploy-mainnet.js --dry-run",
    "stress-test": "ts-node scripts/stress-test-v2.ts",
    "monitor": "ts-node scripts/monitor.ts",
    "monitor:mainnet": "ts-node scripts/monitor.ts --mainnet",
    "pre-deploy-check": "npm run test && npm run build && npm run deploy:mainnet:dry-run"
  },
  "keywords": [
    "ton",
    "blockchain",
    "tact",
    "animal-helper",
    "aht"
  ],
  "author": "",
  "license": "MIT",
  "type": "commonjs",
  "devDependencies": {
    "@tact-lang/compiler": "^1.1.2",
    "@ton/sandbox": "^0.33.0",
    "@ton/test-utils": "^0.10.0",
    "@types/babel__core": "^7.20.5",
    "@types/babel__generator": "^7.27.0",
    "@types/babel__template": "^7.4.4",
    "@types/babel__traverse": "^7.20.7",
    "@types/jest": "^29.5.5",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@ton-community/test-utils": "^0.3.0",
    "@ton/core": "^0.61.0",
    "@ton/crypto": "^3.3.0",
    "@ton/ton": "^15.3.0",
    "@types/node": "^20.8.2",
    "dotenv": "^16.5.0"
  }
}
